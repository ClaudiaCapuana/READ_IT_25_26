RewriteEngine on

# Routes

# Route users.logout


# url users/logout
# pattern : ?users=logout
# RewriteRule ^posts/([1-9][0-9]*)/[a-z0-9][a-z0-9\-]*$ index.php?posts=show&id=$1 [L]

RewriteRule ^users/logout$ index.php?users=logout [L]

#Route users.index
#url users
#pattern ?users=index

RewriteRule  ^users$ index.php?users=index [L]

#Route users.new
#url users
#pattern ?users=new

RewriteRule  ^users/new$ index.php?users=new [L]

RewriteCond %{REQUEST_URI} !\.(js|css|jpg|gif|zip|png|pdf)$ [NC]
RewriteRule ^(.*)$ index.php [L]